<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings xdt:Transform="Insert">
    <add key="owin:appStartup" value="iViewStartup" xdt:Transform="Insert" />
  </appSettings>
  <location path="iView" xdt:Transform="Insert">
    <system.webServer>
      <handlers>
        <add name="iViewFileHandler" path="*" verb="*" type="System.Web.StaticFileHandler" />
      </handlers>
    </system.webServer>
  </location>
</configuration>